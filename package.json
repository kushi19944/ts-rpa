{
  "name": "ts-rpa",
  "version": "0.0.1-rc.5",
  "description": "npm install -g ts-node",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "src/**/*.ts"
  ],
  "bin": {
    "ts-rpa": "dist/bin.js"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "bin": "ts-node src/bin.ts",
    "build": "rimraf ./dist && require-self && tsc",
    "prepare": "npm run build",
    "lint": "tslint 'src/**/*.ts'",
    "fix": "tslint --fix 'src/**/*.ts'",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "test": "echo \"Error: no test specified\""
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^11.11.1",
    "log4js": "^4.0.2",
    "require-self": "^0.2.1",
    "rimraf": "^2.6.3",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "pre-commit": "^1.2.2",
    "prettier": "1.16.4",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tsutils": "^3.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ca-rpa/ts-rpa.git"
  },
  "bugs": {
    "url": "https://github.com/ca-rpa/ts-rpa/issues"
  },
  "homepage": "https://github.com/ca-rpa/ts-rpa#readme"
}
